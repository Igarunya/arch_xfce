<!--
covergloobus skin with no covers!
seekbar images are from Simple by Mustapha Asbbar, http://alezzacreative.deviantart.com
-->

  <skin name="eOS" width="300" height="250">
<albumcover x="10" y="65" width="65" height="65" round="0"/>
  <artistname x="0" y="20" width="300" font="Raleway Light 18" align="center" color="#ffffffff"  maxchars="55" scroll="true" />
  
   <playercontrols spacing="0" x="0" y="0">
		<prev x="90" y="80" src="prev.png" hover="prev.over.png" />
		<play x="130" y="80" src="pause.png" hover="pause.over.png" pause="play.png" pause_hover="play.over.png" />
		<next x="180" y="80" src="next.png" hover="next.over.png" /> 

	</playercontrols>
  <seekbar x="0" y="58" width="300" height="1" back="seekbar.back.png" top="seekbar.top.png">
  	<seekbar x="20" y="187" width="149" height="2" back="seekbar.back.png" top="seekbar.top.png">
   	</seekbar>
  </seekbar>
  <titlename x="0" y="135" width="300" font="Raleway 12" align="center" color="#ffffffff"  maxchars="55" scroll="true" />
  </skin>

